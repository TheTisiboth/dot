# Copy this to .env for Docker setup
# Docker Environment Configuration for Ultimate Frisbee Bot

# Required: Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
CHAT_ID=your_chat_id_here

# Ollama is automatically configured in docker-compose
# OLLAMA_ENABLED=true
# OLLAMA_HOST=http://ollama:11434
# OLLAMA_MODEL=llama3.2:3b

# Optional: Season Configuration (defaults in docker-compose)
WINTER_START_DATE=9:15
WINTER_LOCATION=Park Arena
WINTER_PRACTICE_DAYS=2:20:30,6:20:30
WINTER_SCHEDULE_TIME=20:00

SUMMER_START_DATE=5:20
SUMMER_LOCATION=Beach Courts
SUMMER_PRACTICE_DAYS=0:19:00,3:19:00
SUMMER_SCHEDULE_TIME=20:00

# Optional: Testing
TEST_MODE=false
OVERRIDE_DATE=